package main

import (
	"fmt"
	"time"
)

func main() {
	WhoStarts("tales", "wagner")
}

func WhoStarts(FirstPlayer string, SecondPlayer string) {

	if time.Now().UnixNano()%2 == 0 {
		fmt.Println(FirstPlayer)
	} else {
		fmt.Println(SecondPlayer)
	}

}

func Game() [][]string {
	var Board [][]string

	for i := 0; i < 3; i++ {
		for j := 0; j < 5; j++ {
			if j%2 == 0 {
				Board[][] = " "
			} else {
				Board[][] = "|"
			}
		}
	}
	return Board
}
